+ * @method undo
+ */
+    this.undo = function() {
+        if (stackPointer === 0) {
+            return this.previous || this;
+        }
+        stackPointer--;
+        if (stackPointer === 0) {
+            slots[0].pop();
+            return this;
+            }
+        restoreState();
+        return this;
+    };
+/* Create an expression from json of a snippet passed
+ * @method createItem
+ * @param string json
+ */
+    this.createItem = function(json) {
+        function findBlank(snippet) {
+            if (Array.isArray(snippet[2])) {
+                snippet[2].forEach(function(a) {
+                    if (Array.isArray(a)) {
+                        findBlank(a);
+                    }
