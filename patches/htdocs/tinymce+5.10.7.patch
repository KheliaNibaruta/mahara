+    function toMathML(element) {
+        if (typeof element !== "object") { return element; }
+        var str = '';
+        element.forEach(function(m) {
+            var attr;
+            if (typeof m !== "object") { return; }
+            str += '<' + m[0];
+            if (m[1] && (typeof m[1] === "object")) {
+                for (attr in m[1]) {
+                    if (typeof m[1][attr] !== "object") {
+                        str += " " + attr + '="' + m[1][attr] + '"';
+                    }
+                }
+            }
+            str += '>';
+            if (m[2]) {
+                str += toMathML(m[2]);
+            }
+            str += '</' + m[0] + '>';
+        });
+        return str;
