+                    return s;
+                }
+                if (typeof s[1] === 'undefined') {
+                    return '';
+                }
+                var o = '';
+                if (typeof s[1].tex !== 'undefined') {
+                    s[1].tex.forEach(function(t) {
+                        if (typeof t === 'string') {
+                            o += t;
+                        }
+                        else {
+                            o += title(s[2][t]);
+                        }
+                    });
+                    return o;
+                }
+                if (typeof s[2] === 'string') {
+                    return s[2];
+                }
+                if (typeof s[2] === 'undefined') {
